{
  "schemaVersion": 1,
  "id": "${mod_id}",
  "version": "${version}",
  "name": "Warp Client",
  "description": "Optimization Client Mod For Fabric",
  "authors": [
    "copecone", "noMoment"
  ],
  "contact": {
    "homepage": "https://github.com/copecone",
    "issues": "https://github.com/copecone/warp/issues",
    "sources": "https://github.com/copecone/warp"
  },
  "license": "MIT License",
  "icon": "assets/${mod_id}/textures/icon.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      {
        "adapter": "kotlin",
        "value": "io.github.copecone.${mod_id}.WarpMod"
      }
    ]
  },

  "mixins": [
    "${mod_id}.mixins.json"
  ],

  "depends": {
    "fabricloader": ">=${fabricloader}",
    "fabric": ">=${fabric_api}",
    "fabric-language-kotlin": ">=${fabric_language_kotlin}",
    "minecraft": ">=${minecraft}",
    "java": ">=${java}"
  }
}